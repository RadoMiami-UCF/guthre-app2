<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dialogboxes.AddItem">
   <children>
      <StackPane layoutX="25.0" layoutY="15.0" prefWidth="350.0">
         <children>
            <TextField maxWidth="-Infinity" minWidth="-Infinity" prefWidth="250.0" StackPane.alignment="CENTER_RIGHT" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Item Name:" StackPane.alignment="CENTER_LEFT" />
         </children>
      </StackPane>
      <StackPane layoutX="25.0" layoutY="45.0" prefWidth="350.0">
         <children>
            <TextField maxWidth="-Infinity" minWidth="-Infinity" prefWidth="250.0" StackPane.alignment="CENTER_RIGHT" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Serial Number:" StackPane.alignment="CENTER_LEFT" />
         </children>
      </StackPane>
      <StackPane layoutX="25.0" layoutY="75.0" prefWidth="350.0">
         <children>
            <TextField maxWidth="-Infinity" minWidth="-Infinity" prefWidth="250.0" StackPane.alignment="CENTER_RIGHT" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Value:" StackPane.alignment="CENTER_LEFT" />
         </children>
      </StackPane>
      <StackPane layoutX="50.0" layoutY="150.0" prefWidth="300.0">
         <children>
            <Button mnemonicParsing="false" prefWidth="100.0" text="Add Item" StackPane.alignment="CENTER_RIGHT" />
            <Button mnemonicParsing="false" prefWidth="100.0" text="Cancel" StackPane.alignment="CENTER_LEFT" />
         </children>
      </StackPane>
   </children>
</AnchorPane>
