<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Text?>

<StackPane alignment="TOP_CENTER" prefHeight="100.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dialogboxes.CustomizableError">
   <children>
      <Text fx:id="errorField" strokeType="OUTSIDE" strokeWidth="0.0" text="Error: This is an example of an error." translateY="10.0" />
      <StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="100.0" translateY="-25.0" StackPane.alignment="BOTTOM_CENTER">
         <children>
            <Button fx:id="continueButton" mnemonicParsing="false" onAction="#closeDialogBox" prefWidth="100.0" text="Continue" />
         </children>
      </StackPane>
   </children>
   <padding>
      <Insets left="20.0" right="20.0" />
   </padding>
</StackPane>
